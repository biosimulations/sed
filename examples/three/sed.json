{
  "inputs": {
    "models": {
      "fba_model": {
        "location": "./mod1_fba.xml",
        "language": "urn:sedml:language:sbml"
      },
      "ode_model": {
        "location": "./mod1_ode.xml",
        "language": "urn:sedml:language:sbml"
      }
    }
  },
  "tasks": {
    "cosimulator": {
      "_type": "cosimulation",
      "simulations": [
        "#tasks:ode",
        "#tasks:fba"
      ],
      "updates": [
        "#tasks:update_from_fba",
        "#tasks:update_from_ode"
      ],
      "loop_variable": "time",
      "range": {
        "start": 0.0,
        "interval": 1.0,
        "numberOfSteps": 50
      },
      "outputLabels": [
        "time",
        "S1",
        "S2",
        "S3",
        "J0",
        "J1",
        "J2"
      ],
      "outputSources": [
        "time",
        "#tasks:ode:model['S1']",
        "#tasks:ode:model['S2']",
        "#tasks:ode:model['S3']",
        "#tasks:fba:model['J0']",
        "#tasks:fba:model['J1']",
        "#tasks:fba:model['J2']"
      ],
      "notes": "Can't use outputVariables in ode/fba, and reference them with ode['S1'], because nothing outputs t0.  Instead, we have to directly check model variables with #tasks:ode:model['var']."
    },
    "ode": {
      "_type": "oneStep",
      "initialModel": "model1",
      "initialTime": "#tasks:cosimulator:loop_value",
      "step": "#tasks:cosimulator:range:interval",
      "outputVariables": [],
      "outputModel": "true"
    },
    "fba": {
      "_type": "fluxBalanceAnalysis",
      "model": "#tasks:paramScan_k2:models",
      "outputVariables": [],
      "outputModel": "true"
    },
    "update_from_ode": {
      "_type": "modelUpdate",
      "model": "#tasks:fba:model",
      "changes": {
        "S1": {
          "_type": "change_value",
          "identifier": "S1",
          "value": "#tasks:ode:model['S1']"
        },
        "S2": {
          "_type": "change_value",
          "identifier": "S2",
          "value": "#tasks:ode:model['S2']"
        },
        "S3": {
          "_type": "change_value",
          "identifier": "S3",
          "value": "#tasks:ode:model['S3']"
        }
      }
    },
    "update_from_fba": {
      "_type": "modelUpdate",
      "model": "#tasks:ode:model",
      "changes": {
        "J0": {
          "_type": "change_value",
          "identifier": "J0",
          "value": "#tasks:fba:model['J0']"
        },
        "J1": {
          "_type": "change_value",
          "identifier": "J1",
          "value": "#tasks:fba:model['J1']"
        },
        "J2": {
          "_type": "change_value",
          "identifier": "J2",
          "value": "#tasks:fba:model['J2']"
        }
      }
    }
  },
  "outputs": {
    "reports": {
      "cosim_out": {
        "outputs": "#tasks:cosimulator",
        "filename": "cosimulation.csv",
        "file_format": "http://purl.org/NET/mediatypes/text/csv"
      }
    },
    "plots": {
      "Fig1": {
        "_type": "Plot2D",
        "label": "Figure 1",
        "xAxis": {
          "label": "Time"
        },
        "yAxis": {
          "label": "Concentrations"
        },
        "curves": {
          "S1": {
            "x": "#tasks:cosimulator['time']",
            "y": "#tasks:cosimulator['S1']"
          },
          "S2": {
            "x": "#tasks:cosimulator['time']",
            "y": "#tasks:cosimulator:['S2']"
          },
          "S3": {
            "x": "#tasks:cosimulator['time']",
            "y": "#tasks:cosimulator:['S3']"
          },
          "J0": {
            "x": "#tasks:cosimulator['time']",
            "y": "#tasks:cosimulator:['J0']"
          },
          "J1": {
            "x": "#tasks:cosimulator['time']",
            "y": "#tasks:cosimulator:['J1']"
          },
          "J2": {
            "x": "#tasks:cosimulator['time']",
            "y": "#tasks:cosimulator:['J2']"
          }
        }
      }
    }
  }
}
