{
  "inputs": {
    "models": {
      "model1": {
        "location": "example.xml",
        "language": "urn:sedml:language:sbml"
      }
    }
  },
  "tasks": {
    "paramScan_k1": {
      "_type": "parameterScan",
      "model": "model1",
      "scannedVariable": "k1",
      "range": {
        "start": 0.1,
        "end": 1.5,
        "interval": 0.1
      },
      "outputRange": "true"
    },
    "paramScan_k2": {
      "_type": "parameterScan",
      "model": "tasks:paramScan_k1:models",
      "scannedVariable": "k2",
      "range": {
        "values": [
          0.1,
          0.2,
          0.5,
          1.0,
          2.0,
          5.0,
          10.0
        ]
      },
      "outputRange": "true"
    },
    "steadyStates": {
      "_type": "steadyState",
      "model": "tasks:paramScan_k2:models",
      "outputVariables": [
        "S1",
        "S2"
      ],
      "outputModel": "true"
    },
    "jacobians": {
      "_type": "calculation",
      "math": "jacobian(tasks:steadyStates:model)"
    }
  },
  "outputs": {
    "reports": {
      "steadyStateReport": {
        "dataSets": "tasks:steadyStates"
      },
      "jacobianReport": {
        "dataSets": "tasks:jacobians"
      }
    },
    "plots": {
      "Fig1": {
        "_type": "plot3D",
        "label": "Figure 1",
        "xAxis": {
          "label": "k1",
          "scale": "linear"
        },
        "yAxis": {
          "label": "k2",
          "scale": "log"
        },
        "zAxis": {
          "label": "Concentrations",
          "scale": "linear"
        },
        "surfaces": {
          "S1": {
            "x": "tasks:paramScan_k1:range",
            "y": "tasks:paramScan_k2:range",
            "z": "tasks:steadyStates['S1']"
          },
          "S2": {
            "x": "tasks:paramScan_k1:range",
            "y": "tasks:paramScan_k2:range",
            "z": "tasks:steadyStates['S2']"
          }
        }
      }
    }
  }
}
